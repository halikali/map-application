"use strict";var eventsArr=[];navigator.geolocation&&navigator.geolocation.getCurrentPosition(function(o){var t=o.coords,o=t.latitude,t=t.longitude;console.log("https://www.google.com/maps/@".concat(o,",").concat(t));var t=[o,t],n=L.map("map").setView(t,13);console.log(n),L.tileLayer("https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",{attribution:'&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors'}).addTo(n),n.on("click",function(o){console.log("mapEvents => ",o);var t=o.latlng,o=t.lat,t=t.lng;console.log("lat,lng => ",o,t),L.marker([o,t]).addTo(n).bindPopup("Seçilen nokta burası => ".concat(o," ").concat(t)).openPopup(),eventsArr.push({lat:o,lng:t}),console.log(eventsArr)})},function(){alert("could not get your position.")});